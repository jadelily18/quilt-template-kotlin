{
	"schema_version": 1,
	"quilt_loader": {
		"group": "com.lilydev",
		"id": "template_mod",
		"version": "${version}",
		"metadata": {
			"name": "Template Mod",
			"description": "This is a template mod!",
			"contributors": {
				"jadelily18": "Owner"
			},
			"contact": {
				"homepage": "https://lilydev.com/",
				"issues": "https://github.com/jadelily18/quilt-template-kotlin/issues",
				"sources": "https://github.com/jadelily18/quilt-template-kotlin"
			},
			"license": "LGPL-3.0",
			"icon": "assets/example_mod/icon.png"
		},
		"intermediate_mappings": "net.fabricmc:intermediary",
		"entrypoints": {
			"init": {
				"adapter": "kotlin",
				"value": "com.lilydev.example_mod.ExampleMod"
			},
			"modmenu": {
				"adapter": "kotlin",
				"value": "com.lilydev.example_mod.integrations.ModMenuImpl"
			}
		},
		"depends": [
			{
				"id": "quilt_loader",
				"versions": ">=0.17.0-"
			},
			{
				"id": "quilted_fabric_api",
				"versions": ">=4.0.0-"
			},
			{
				"id": "minecraft",
				"versions": ">=1.19.2"
			},
			{
				"id": "qkl",
				"versions": "0.1.2-MODFEST"
			}
		]
	},
	"modmenu": {
		"links": {
			"modmenu.discord": "https://discord.gg/TZAt4PA5av",
			"modmenu.kofi": "https://ko-fi.com/jadelily"
		}
	},
	"mixin": "example_mod.mixins.json"
}
